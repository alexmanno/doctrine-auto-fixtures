{
    "name": "alexmanno/doctrine-auto-fixtures",
    "description": "Automatic fixture for doctrine entities",
    "type": "library",
    "authors": [
        {
            "name": "Alessandro Manno",
            "email": "alessandromanno96@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlexManno\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexManno\\Tests\\": "tests"
        }
    },
    "require": {
        "doctrine/annotations": "^1.6",
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "facile-it/facile-coding-standard": "^0.2.0",
        "phpstan/phpstan": "^0.9.2"
    },
    "scripts": {
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix --diff"
    }
}
